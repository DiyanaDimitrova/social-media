{
  "name": "social-media-task",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "install-server": "cd server && npm install",
    "install-client": "cd client && npm install && bower install",
    "install-packages": "concurrently \"npm run install-server\" \"npm run install-client\"",
    "client": "cd client && gulp serve",
    "server": "cd server && nodemon index.js",
    "start": "concurrently \"npm run server\" \"npm run client\" "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DiyanaDimitrova/social-media-task.git"
  },
